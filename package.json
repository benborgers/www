{
  "private": true,
  "name": "www",
  "description": "",
  "license": "",
  "scripts": {
    "build": "cross-env NODE_ENV=production node scripts/posts-to-json && npm run build:css && remix build",
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "postinstall": "remix setup cloudflare-pages",
    "dev:remix": "remix watch",
    "dev:wrangler": "wrangler pages dev ./public --binding $(cat .env)",
    "dev:css": "npx tailwindcss -o app/generated/tailwind-build.css --watch",
    "build:css": "npx tailwindcss -o app/generated/tailwind-build.css --minify"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.5",
    "@radix-ui/react-slider": "^0.1.3",
    "@remix-run/cloudflare-pages": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^2.23.0",
    "@tailwindcss/typography": "^0.5.0",
    "framer-motion": "^5.5.5",
    "gray-matter": "^4.0.3",
    "katex": "^0.15.1",
    "luxon": "^2.2.0",
    "markdown-it": "^12.3.2",
    "prismjs": "^1.26.0",
    "puppeteer": "^13.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redis": "^4.0.1",
    "remix": "^1.1.1",
    "tailwindcss": "^3.0.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.3.0",
    "@remix-run/dev": "^1.1.1",
    "@types/luxon": "^2.0.8",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.11",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.5.4",
    "wrangler": "^0.0.7"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}
