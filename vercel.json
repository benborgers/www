{
  "redirects": [
    {
      "source": "/blog/(.*?)",
      "destination": "/posts/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/swipes/(?<path>.*)",
      "destination": "https://mealswipes.netlify.app/$path"
    },
    {
      "source": "/slowmochristian/(?<path>.*)",
      "destination": "https://slowmochristian.netlify.app/$path"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "github": { "silent": true }
}
