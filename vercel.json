{
  "redirects": [
    {
      "source": "/blog",
      "destination": "/posts",
      "permanent": true
    },
    {
      "source": "/blog/(.*?)",
      "destination": "/posts/$1",
      "permanent": true
    },
    {
      "source": "/posts",
      "destination": "/technical-posts",
      "permanent": true,
      "has": [
        {
          "type": "query",
          "key": "technical"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "github": { "silent": true }
}
