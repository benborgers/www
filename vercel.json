{
  "redirects": [
    {
      "source": "/blog",
      "destination": "/posts",
      "permanent": true
    },
    {
      "source": "/blog/(.*?)",
      "destination": "/posts/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/swipes",
      "destination": "https://mealswipes.netlify.app"
    },
    {
      "source": "/swipes/(.*)",
      "destination": "https://mealswipes.netlify.app/$1"
    },
    {
      "source": "/slowmochristian",
      "destination": "https://slowmochristian.netlify.app"
    },
    {
      "source": "/slowmochristian/(.*)",
      "destination": "https://slowmochristian.netlify.app/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "github": { "silent": true }
}
