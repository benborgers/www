{
  "headers": [
    {
      "source": "/rss",
      "headers": [{ "key": "Content-Type", "value": "application/rss+xml" }]
    },
    {
      "source": "/me.jpg",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=86400, immutable" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/rss.xml",
      "destination": "/rss",
      "permanent": true
    },
    {
      "source": "/slowmochristian",
      "destination": "http://slowmochristian.ben.page",
      "permanent": true
    },
    {
      "source": "/swipes",
      "destination": "http://swipes.ben.page",
      "permanent": true
    },
    {
      "source": "/blog",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/blog/([^/]+)",
      "destination": "/$1",
      "permanent": true
    },
    {
      "source": "/posts/([^/]+)",
      "destination": "/$1",
      "permanent": true
    }
  ]
}
