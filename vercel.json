{
  "redirects": [
    {
      "source": "/blog/:slug.*",
      "destination": "/posts/:slug",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/swipes/:path.*",
      "destination": "https://mealswipes.netlify.app/$1"
    },
    {
      "source": "/slowmochristian/:path.*",
      "destination": "https://slowmochristian.netlify.app/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "github": { "silent": true }
}
